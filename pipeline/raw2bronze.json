{
	"name": "raw2bronze",
	"properties": {
		"activities": [
			{
				"name": "Copy data to bronze",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"wildcardFileName": "*.json",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings",
							"copyBehavior": "FlattenHierarchy"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['movie'][0]['Id']"
								},
								"sink": {
									"name": "Id"
								}
							},
							{
								"source": {
									"path": "$['movie'][0]['Title']"
								},
								"sink": {
									"name": "Title"
								}
							},
							{
								"source": {
									"path": "$['movie'][0]['Overview']"
								},
								"sink": {
									"name": "Overview"
								}
							},
							{
								"source": {
									"path": "$['movie'][0]['Tagline']"
								},
								"sink": {
									"name": "Tagline"
								}
							},
							{
								"source": {
									"path": "$['movie'][0]['Budget']"
								},
								"sink": {
									"name": "Budget"
								}
							},
							{
								"source": {
									"path": "$['movie'][0]['Revenue']"
								},
								"sink": {
									"name": "Revenue"
								}
							},
							{
								"source": {
									"path": "$['movie'][0]['ImdbUrl']"
								},
								"sink": {
									"name": "ImdbUrl"
								}
							},
							{
								"source": {
									"path": "$['movie'][0]['TmdbUrl']"
								},
								"sink": {
									"name": "TmdbUrl"
								}
							},
							{
								"source": {
									"path": "$['movie'][0]['PosterUrl']"
								},
								"sink": {
									"name": "PosterUrl"
								}
							},
							{
								"source": {
									"path": "$['movie'][0]['BackdropUrl']"
								},
								"sink": {
									"name": "BackdropUrl"
								}
							},
							{
								"source": {
									"path": "$['movie'][0]['OriginalLanguage']"
								},
								"sink": {
									"name": "OriginalLanguage"
								}
							},
							{
								"source": {
									"path": "$['movie'][0]['ReleaseDate']"
								},
								"sink": {
									"name": "ReleaseDate"
								}
							},
							{
								"source": {
									"path": "$['movie'][0]['RunTime']"
								},
								"sink": {
									"name": "RunTime"
								}
							},
							{
								"source": {
									"path": "$['movie'][0]['Price']"
								},
								"sink": {
									"name": "Price"
								}
							},
							{
								"source": {
									"path": "$['movie'][0]['CreatedDate']"
								},
								"sink": {
									"name": "CreatedDate"
								}
							},
							{
								"source": {
									"path": "$['movie'][0]['UpdatedDate']"
								},
								"sink": {
									"name": "UpdatedDate"
								}
							},
							{
								"source": {
									"path": "$['movie'][0]['UpdatedBy']"
								},
								"sink": {
									"name": "UpdatedBy"
								}
							},
							{
								"source": {
									"path": "$['movie'][0]['CreatedBy']"
								},
								"sink": {
									"name": "CreatedBy"
								}
							},
							{
								"source": {
									"path": "$['movie'][0]['genres'][0]['id']"
								},
								"sink": {
									"name": "id"
								}
							},
							{
								"source": {
									"path": "$['movie'][0]['genres'][0]['name']"
								},
								"sink": {
									"name": "name"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "movie_blob",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DataFactoryOut",
						"type": "DatasetReference",
						"parameters": {
							"tableName": "bronze"
						}
					}
				]
			}
		],
		"annotations": []
	}
}